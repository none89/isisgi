import java util.stack;
public class main {
    public static void main(string[] args){
        Stack<String> pilhaCarros = new Stack<>();
pilhaCarros.push("chevet"); 
pilhaCarros.push("impala");
pilhaCarros.push("ferrari");
carroremovido=pilhacarros.pop();
carrotopo=pilhacarros.peek();
